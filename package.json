{
  "name": "@eox/ui",
  "version": "0.0.8",
  "description": "A curated collection of vue.js UI components, EOX style.",
  "author": "Silvester Pari",
  "repository": "https://github.com/silvester-pari/eox-ui",
  "keywords": [
    "user interface",
    "vue.js"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:npm": "vue-cli-service build --target lib --name eox ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/eox.umd.js",
  "unpkg": "./dist/eox.umd.min.js",
  "dependencies": {
    "core-js": "^2.6.5",
    "marked": "^0.6.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
